{
    "name": "Xilinx ZLIB 8 Parallel Bytes Decompress with Loop II=1",
    "description": [
        "Xilinx ZLIB Decompression Parallel Byte 8_II1"
    ],
    "flow": "vitis",
    "platform_blacklist": [
        "u50",
        "u280"
    ],
    "platform_whitelist": [
        "u200",
        "u250"
    ],

    "launch": [
        {
            "cmd_args": "-dx BUILD/decompress_stream.xclbin -d PROJECT/sample.txt.zlib",
            "name": "generic launch for all flows"
        }
    ],
    "host": {
        "host_exe": "xil_zlib",
        "compiler": {
            "sources": [
                "./src/host.cpp",
                "LIB_DIR/L2/tests/src/zlib.cpp",
                "LIB_DIR/common/libs/xcl2/xcl2.cpp",
                "LIB_DIR/common/libs/cmdparser/cmdlineparser.cpp",
                "LIB_DIR/common/libs/logger/logger.cpp",
                "LIB_DIR/common/thirdParty/xxhash/xxhash.c"
            ],
            "includepaths": [
                "LIB_DIR/L2/include",
                "LIB_DIR/L1/include/hw",
                "LIB_DIR/L2/tests/src"
            ],
            "symbols": [
                "PARALLEL_BLOCK=1",
                "D_COMPUTE_UNIT=1",
                "INPUT_BUFFER_SIZE=128*1024*1024",
                "OUTPUT_BUFFER_SIZE=128*1024*1024"
            ]
        }
    },
    "v++": {
        "build_datafiles" : [
            "PROJECT/advanced.ini",
            "PROJECT/hls_directives.tcl"
        ],
        "compiler": {
            "symbols": [
                "PARALLEL_BLOCK=1",
                "MULTIPLE_BYTES=8"
            ],
            "clflags": [
                "--config PROJECT/advanced.ini"
            ]
        }
    },
    "containers": [
        {
            "name": "decompress_stream",
            "ldclflags": "--config PROJECT/opts.ini --vivado.prop run.impl_1.STEPS.ROUTE_DESIGN.TCL.POST=PROJECT/post_route.tcl",
            "accelerators": [
                {
                    "name": "xilZlibDmReader",
                    "location": "LIB_DIR/L2/tests/src/zlib_dm_multibyte_rd.cpp",
                    "compute_units": [
                        {
                            "slr": "SLR0",
                            "arguments": [   
                                {
                                    "name": "out",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "encoded_size",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "status_flag",
                                    "memory": "DDR[0]"
                                }
                            ]
                        }
                    ]
                },
                {
                    "name": "xilZlibDmWriter",
                    "location": "LIB_DIR/L2/tests/src/zlib_dm_multibyte_wr.cpp",
                    "compute_units": [
                        {
                            "slr": "SLR0",
                            "arguments": [
                                {
                                    "name": "in",
                                    "memory": "DDR[0]"
                                }
                            ]
                        }
                    ]
                },
                {
                    "name": "xilDecompressStream",
                    "location": "LIB_DIR/L2/src/zlib_parallelbyte_decompress_stream.cpp",
                    "compute_units": [
                        {
                            "slr": "SLR0"
                        }
                    ]
                }
            ]
        }
    ],
    "output_files" : "sample.txt.zlib.*",
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": 300
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw"
        ],
        "category": "canary"
    }
}
