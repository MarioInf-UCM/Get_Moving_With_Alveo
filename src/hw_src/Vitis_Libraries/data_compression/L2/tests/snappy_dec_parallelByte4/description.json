{
    "name": "Xilinx Snappy MultiByte-4 Decompress",
    "description": [
        "Xilinx Snappy MultiByte Decompress:4 parallel bytes processed at a time"
    ],
    "flow": "vitis",
    "launch": [
        {
            "cmd_args": "-dx BUILD/decompress.xclbin -d PROJECT/sample.txt.snappy",
            "name": "generic launch for all flows"
        }
    ],
    "platform_blacklist": [
        "u280"
    ],
    "platform_whitelist": [
        "u200",
        "u250"
   ],
    "host": {
        "host_exe": "xil_snappy",
        "compiler": {
            "sources": [
                "./src/host.cpp",
                "LIB_DIR/L2/tests/src/snappy.cpp",
                "LIB_DIR/common/libs/xcl2/xcl2.cpp",
                "LIB_DIR/common/libs/cmdparser/cmdlineparser.cpp",
                "LIB_DIR/common/libs/logger/logger.cpp",
                "LIB_DIR/common/thirdParty/xxhash/xxhash.c"
            ],
            "includepaths": [
                "LIB_DIR/L2/tests/src/"
            ],
            "symbols": [
                "PARALLEL_BLOCK=1"
            ]
        }
    },
    "v++": {
        "compiler": {
            "symbols": [
                "PARALLEL_BLOCK=1",
                "PARALLEL_BYTE=4"
            ]
        }
    },
    "containers": [
        {
            "name": "decompress",
            "accelerators": [
                {
                    "name": "xilSnappyDecompress",
                    "location": "LIB_DIR/L2/src/snappy_multibyte_decompress_mm.cpp"
                }
            ]
        }
    ],
    "output_files" : "sample.txt.snappy.*",
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": 300
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw"
        ],
        "category": "canary"
    }
}
