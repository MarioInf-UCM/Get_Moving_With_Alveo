{
    "gui": true,
    "name": "Xilinx Shortest Path Test", 
    "description": "", 
    "flow": "vitis", 
    "platform_whitelist": [
        "u200",
        "u250"
    ], 
    "platform_blacklist": [
        "zc"
    ],
    "platform_properties": {
        "u200": {
	    "v++": {
	        "compiler": {
		    "cflags": [
		        "--config PROJECT/conn_u200_u250.ini"
		    ]
		}
	    }
	},
        "u250": {
	    "v++": {
	        "compiler": {
		    "cflags": [
		        "--config PROJECT/conn_u200_u250.ini"
		    ]
		}
	    }
	}
    },
    "launch": [
        {
            "cmd_args": " -xclbin BUILD/shortestPath_top.xclbin -o PROJECT/data/data_offset.csr -c PROJECT/data/data_column.csr -w PROJECT/data/data_weight.csr -g PROJECT/data/data.mtx.sssp", 
            "name": "generic launch for all flows"
        }
    ], 
    "host": {
        "host_exe": "host.exe", 
        "compiler": {
            "sources": [
                "LIB_DIR/L2/tests/shortest_path/host/main.cpp", 
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ], 
            "includepaths": [
                "LIB_DIR/L2/include", 
                "LIB_DIR/L2/tests/shortest_path/host", 
                "LIB_DIR/L2/tests/shortest_path/kernel", 
                "LIB_DIR/ext/xcl2"
            ], 
            "options": "-O3 "
        }
    }, 
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L2/include",
                "LIB_DIR/L2/tests/shortest_path/kernel",
                "LIB_DIR/../database/L1/include/hw",
                "LIB_DIR/../quantitative_finance/L1/include",
		        "LIB_DIR/../utils/L1/include"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "LIB_DIR/L2/tests/shortest_path/kernel/shortestPath_top.cpp", 
                    "frequency": 300.0, 
                    "clflags": " -D KERNEL_NAME=shortestPath_top", 
                    "name": "shortestPath_top",
		    "num_compute_units": 1,
		    "compute_units": [
                        {
                            "name": "shortestPath_top",
                            "slr": "SLR0",
                            "arguments": [
                                {
                                    "name": "config",
                                    "memory": "DDR[0]"
                                },
				{
                                    "name": "offset",
                                    "memory": "DDR[0]"
                                },
				{
                                    "name": "column",
                                    "memory": "DDR[0]"
                                },
				{
                                    "name": "weight",
                                    "memory": "DDR[0]"
                                },
				{
                                    "name": "ddrQue512",
                                    "memory": "DDR[0]"
                                },
				{
                                    "name": "ddrQue",
                                    "memory": "DDR[0]"
                                },
				{
                                    "name": "result512",
                                    "memory": "DDR[0]"
                                },
				{
                                    "name": "result",
                                    "memory": "DDR[0]"
                                },
				{
                                    "name": "info",
                                    "memory": "DDR[0]"
                                }
                            ]
                        }
                    ]
                }
            ], 
            "frequency": 300.0, 
            "name": "shortestPath_top"
        }
    ], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": 32768, 
                "max_time_min": 300
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw"
        ], 
        "category": "canary"
    }
}
