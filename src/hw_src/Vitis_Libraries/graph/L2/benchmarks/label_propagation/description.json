{
    "gui": true,
    "name": "Xilinx Label Propagation Benchmark", 
    "description": "", 
    "flow": "vitis", 
    "platform_whitelist": [
        "u200",
	"u250"
    ], 
    "platform_blacklist": [
        "zc"
    ],
    "platform_properties": {
        "u200": {
	    "v++": {
	        "compiler": {
		    "cflags": [
		        "--config PROJECT/conn_u200_u250.ini"
		    ]
		}
	    }
	},
        "u250": {
	    "v++": {
	        "compiler": {
		    "cflags": [
		        "--config PROJECT/conn_u200_u250.ini"
		    ]
		}
	    }
	}
    },
    "launch": [
        {
            "cmd_args": " -xclbin BUILD/LPKernel.xclbin -o PROJECT/data/csr_offsets.txt -i PROJECT/data/csr_columns.txt -label PROJECT/data/label.txt", 
            "name": "generic launch for all flows"
        }
    ], 
    "host": {
        "host_exe": "host.exe", 
        "compiler": {
            "sources": [
                "LIB_DIR/L2/benchmarks/label_propagation/host/main.cpp", 
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ], 
            "includepaths": [
                "LIB_DIR/L2/include", 
                "LIB_DIR/L2/benchmarks/label_propagation/host", 
                "LIB_DIR/L2/benchmarks/label_propagation/kernel", 
                "LIB_DIR/ext/xcl2",
                "LIB_DIR/../database/L1/include/hw",
                "LIB_DIR/../quantitative_finance/L1/include",
		        "LIB_DIR/../utils/L1/include"
            ], 
            "options": "-O3 "
        }
    }, 
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L2/include", 
                "LIB_DIR/L2/benchmarks/label_propagation/kernel",
                "LIB_DIR/../database/L1/include/hw",
                "LIB_DIR/../quantitative_finance/L1/include",
		        "LIB_DIR/../utils/L1/include"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "LIB_DIR/L2/benchmarks/label_propagation/kernel/label_propagation_top.cpp", 
                    "frequency": 300.0, 
                    "clflags": " -D KERNEL_NAME=LPKernel", 
                    "name": "LPKernel",
		    "num_compute_units": 1,
                    "compute_units": [
                        {
                            "name": "LPKernel",
                            "slr": "SLR0",
                            "arguments": [
                                {
                                    "name": "offsetCSR",
                                    "memory": "DDR[0]"
                                },
				{
                                    "name": "indexCSR",
                                    "memory": "DDR[0]"
                                },
				{
                                    "name": "offsetCSC",
                                    "memory": "DDR[0]"
                                },
				{
                                    "name": "indexCSC",
                                    "memory": "DDR[0]"
                                },
				{
                                    "name": "indexCSC2",
                                    "memory": "DDR[0]"
                                },
				{
                                    "name": "pingHashBuf",
                                    "memory": "DDR[0]"
                                },
				{
                                    "name": "pongHashBuf",
                                    "memory": "DDR[0]"
                                },
				{
                                    "name": "labelPing",
                                    "memory": "DDR[0]"
                                },
				{
                                    "name": "labelPong",
                                    "memory": "DDR[0]"
                                }
                            ]
                        }
                    ]
                }
            ], 
            "frequency": 300.0, 
            "name": "LPKernel"
        }
    ], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": 32768, 
                "max_time_min": 300
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw"
        ], 
        "category": "canary"
    }
}
