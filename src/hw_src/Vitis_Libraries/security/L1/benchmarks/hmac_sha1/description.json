{
    "gui": true,
    "name": "Xilinx HMAC-SHA1 Benchmark", 
    "description": "", 
    "flow": "vitis", 
    "platform_type": "pcie", 
    "platform_whitelist": [
        "xilinx_u250_xdma_201830_2"
    ], 
    "platform_blacklist": [
        "zc"
    ], 
    "platform_properties": {
        "u250": {
            "host": {
                "compiler": {
                    "symbols": [
                        "USE_DDR"
                    ]
                }
            }, 
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u250.ini"
                    ]
                }
            }
        }
    }, 
    "launch": [
        {
            "cmd_args": " -mode fpga -xclbin BUILD/hmacSha1Kernel.xclbin", 
            "name": "generic launch for all flows"
        }
    ], 
    "host": {
        "host_exe": "hmacSha1Benchmark.exe", 
        "compiler": {
            "sources": [
                "host/main.cpp", 
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ], 
            "includepaths": [
                "LIB_DIR/L1/include", 
                "host", 
                "kernel", 
                "LIB_DIR/ext/xcl2"
            ], 
            "options": "-O3 -lssl -lcrypto"
        }
    }, 
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include", 
                "kernel"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "kernel/hmacSha1Kernel1.cpp", 
                    "frequency": 300.0, 
                    "name": "hmacSha1Kernel_1",
                    "num_compute_units": 1,
                    "compute_units" : [
                        {
                            "name": "hmacSha1Kernel_1",
                            "slr": "SLR0",
                            "arguments": [
                                {
                                    "name": "inputData",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "outputData",
                                    "memory": "DDR[0]"
                                }
                            ]
                        }
                    ]
                }, 
                {
                    "location": "kernel/hmacSha1Kernel2.cpp", 
                    "frequency": 300.0, 
                    "name": "hmacSha1Kernel_2",
                    "num_compute_units": 1,
                    "compute_units" : [
                        {
                            "name": "hmacSha1Kernel_2",
                            "slr": "SLR1",
                            "arguments": [
                                {
                                    "name": "inputData",
                                    "memory": "DDR[1]"
                                },
                                {
                                    "name": "outputData",
                                    "memory": "DDR[1]"
                                }
                            ]
                        }
                    ]
                }, 
                {
                    "location": "kernel/hmacSha1Kernel3.cpp", 
                    "frequency": 300.0, 
                    "name": "hmacSha1Kernel_3",
                    "num_compute_units": 1,
                    "compute_units" : [
                        {
                            "name": "hmacSha1Kernel_3",
                            "slr": "SLR2",
                            "arguments": [
                                {
                                    "name": "inputData",
                                    "memory": "DDR[2]"
                                },
                                {
                                    "name": "outputData",
                                    "memory": "DDR[2]"
                                }
                            ]
                        }
                    ]
                }, 
                {
                    "location": "kernel/hmacSha1Kernel4.cpp", 
                    "frequency": 300.0, 
                    "name": "hmacSha1Kernel_4",
                    "num_compute_units": 1,
                    "compute_units" : [
                        {
                            "name": "hmacSha1Kernel_4",
                            "slr": "SLR3",
                            "arguments": [
                                {
                                    "name": "inputData",
                                    "memory": "DDR[3]"
                                },
                                {
                                    "name": "outputData",
                                    "memory": "DDR[3]"
                                }
                            ]
                        }
                    ]
                }
            ], 
            "frequency": 300.0, 
            "name": "hmacSha1Kernel"
        }
    ], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": 16384, 
                "max_time_min": 420
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw"
        ], 
        "category": "canary"
    }
}
