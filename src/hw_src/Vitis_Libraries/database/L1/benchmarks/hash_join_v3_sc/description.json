{
    "name": "Xilinx Hash Join V3 Benchmark",
    "description": "Xilinx Hash Join V3 Benchmark",
    "flow": "vitis",
    "gui": true,
    "platform_type": "pcie",
    "platform_whitelist": [
        "u280"
    ],
    "platform_blacklist": [
        "zc"
    ],
    "platform_properties": {
        "u280": {
            "host": {
                "compiler": {
                    "symbols": []
                }
            },
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u280.ini"
                    ]
                }
            }
        }
    },
    "launch": [
        {
            "name": "generic launch for sw_emu flows",
            "target": "sw_emu",
            "cmd_args": " -xclbin BUILD/hash_join_v3.xclbin -scale 10"
        },
        {
            "name": "generic launch for hw_emu flows",
            "target": "hw_emu",
            "cmd_args": " -xclbin BUILD/hash_join_v3.xclbin -scale 5000"
        },
        {
            "name": "generic launch for hw flows",
            "target": "hw",
            "cmd_args": " -xclbin BUILD/hash_join_v3.xclbin"
        }
    ],
    "host": {
        "host_exe": "test_join.exe",
        "compiler": {
            "sources": [
                "host/test_join.cpp",
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ],
            "includepaths": [
                "host",
                "LIB_DIR/ext/xcl2",
                "kernel"
            ],
            "options": "-O3 ",
            "symbols": []
        }
    },
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include/hw",
                "kernel",
                "host"
            ]
        }
    },
    "containers": [
        {
            "accelerators": [
                {
                    "location": "kernel/join_kernel.cpp",
                    "frequency": 300,
                    "clflags": " -D KERNEL_NAME=join_kernel",
                    "name": "join_kernel",
                    "num_compute_units": 1,
                    "compute_units": [
                        {
                            "name": "join_kernel",
                            "slr": "SLR0",
                            "arguments": [
                                {
                                    "name": "buf_o_orderkey",
                                    "memory": "DDR[0]"
                                },
                                {
