                                },
                                {
                                    "name": "buf_l_extendedprice",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "buf_l_discount",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "pu0_ht",
                                    "memory": "HBM[0]"
                                },
                                {
                                    "name": "pu1_ht",
                                    "memory": "HBM[4]"
                                },
                                {
