{
    "gui": true,
    "name": "Xilinx Kmeans Training",
    "description": "",
    "flow": "vitis",
    "platform_type": "pcie",
    "platform_whitelist": [
        "u250"
    ], 
    "platform_blacklist": [
        "zc"
    ],
    "platform_properties": {
        "u250": {
            "host": {
                "compiler": {
                    "symbols": [
                        "USE_DDR"
                    ]
                }
            }
        }
    },
    "launch": [
        {
            "cmd_args": "-xclbin BUILD/kmeanskernel.xclbin", 
            "name": "generic launch for all flows"
        }
    ], 
    "host": {
        "host_exe": "hostexe", 
        "compiler": {
            "sources": [
                "host/main.cpp", 
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ], 
            "includepaths": [
                "LIB_DIR/L1/include", 
                "LIB_DIR/L2/include", 
                "LIB_DIR/ext/xcl2",
                "LIB_DIR/../utils/L1/include"
            ], 
            "options": "-O3 "
        }
    }, 
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include", 
                "LIB_DIR/L2/include",
                "LIB_DIR/../utils/L1/include"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "kernel/kernel.cpp", 
                    "frequency": 300.0, 
                    "name": "kmeansKernel",
                    "num_compute_units": 1,
                    "compute_units" : [
                        {
                            "name": "kmeansKernel",
                            "slr": "SLR0",
                            "arguments": [
                                {
                                    "name": "inputData",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "centers",
                                    "memory": "DDR[0]"
                                }
                            ]
                        }
                    ]
                }
            ], 
            "frequency": 300.0, 
            "name": "kmeanskernel"
        }
    ], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": 32768, 
                "max_time_min": 520
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw"
        ], 
        "category": "canary"
    }
}
