{
    "name": "Xilinx Population Based Markov Chain Monte Carlo Model", 
    "description": "L2 Population Based Markov Chain Monte Carlo Model", 
    "flow": "vitis", 
    "platform_type": "pcie", 
    "platform_whitelist": [
        "u250", 
        "u200"
    ], 
    "platform_blacklist": [
        "zc"
    ], 
    "platform_properties": {
        "u200": {
            "host": {
                "compiler": {
                    "symbols": [
                        "USE_DDR"
                    ]
                }
            }, 
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u200.ini"
                    ]
                }
            }
        }, 
        "u250": {
            "host": {
                "compiler": {
                    "symbols": [
                        "USE_DDR"
                    ]
                }
            }, 
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u250.ini"
                    ]
                }
            }
        }
    }, 
    "launch": [
        {
            "cmd_args": " BUILD/mcmc_kernel.xclbin 500 50", 
            "name": "generic launch for all flows"
        }
    ], 
    "host": {
        "host_exe": "mcmc_test.exe", 
        "compiler": {
            "sources": [
                "LIB_DIR/L2/tests/PopMCMC/src/host/mcmc_test.cpp", 
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ], 
            "includepaths": [
                "LIB_DIR/L1/include", 
                "LIB_DIR/L2/include", 
                "LIB_DIR/L2/tests/PopMCMC/src/host", 
                "LIB_DIR/ext/xcl2", 
                "LIB_DIR/L2/tests/PopMCMC/src/kernel"
            ], 
            "options": "-O3 "
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "LIB_DIR/L2/tests/PopMCMC/src/kernel/mcmc_kernel.cpp", 
                    "frequency": 300.0, 
                    "name": "mcmc_kernel"
                }
            ], 
            "frequency": 300.0, 
            "name": "mcmc_kernel"
        }
    ], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": 32768, 
                "max_time_min": 300
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw"
        ], 
        "category": "canary"
    }
}
