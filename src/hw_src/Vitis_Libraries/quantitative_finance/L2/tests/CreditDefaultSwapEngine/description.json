{
    "name": "Xilinx Credit Default Swap Engine Model", 
    "description": "L2 Credit Default Swap Model", 
    "flow": "vitis", 
    "platform_type": "pcie", 
    "platform_whitelist": [
        "u250", 
        "u200"
    ], 
    "platform_blacklist": [
        "zc"
    ], 
    "launch": [
        {
            "cmd_args": " -xclbin BUILD/cds_engine.xclbin", 
            "name": "generic launch for all flows"
        }
    ], 
    "host": {
        "host_exe": "cds_host.exe", 
        "compiler": {
            "includepaths" : [
                "LIB_DIR/L2/tests/CreditDefaultSwapEngine/src/kernel",
                "LIB_DIR/L2/include",
                "LIB_DIR/L1/include",
                "LIB_DIR/ext/xcl2"
            ],
            "sources": [
                "LIB_DIR/L2/tests/CreditDefaultSwapEngine/src/host/main.cpp", 
                "LIB_DIR/L2/tests/CreditDefaultSwapEngine/src/host/cpu.cpp", 
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ], 
            "options": "-O3 ", 
            "symbols": [
                "TEST_DT=float", 
                "DEVICE_PART=u250"
            ]
        }
    }, 
    "v++" : {
        "compiler" : {
            "symbols" : [
                "TEST_DT=float"
            ],
            "includepaths" : [
                "LIB_DIR/L1/include",
                "LIB_DIR/L2/include"
            ]
        }
    },
    "containers": [
        {
            "accelerators": [
                {
                    "location": "LIB_DIR/L2/tests/CreditDefaultSwapEngine/src/kernel/CDS_kernel.cpp", 
                    "frequency": 300.0, 
                    "clflags": " -D TEST_DT=float", 
                    "name": "CDS_kernel"
                }
            ], 
            "frequency": 300.0, 
            "name": "cds_engine"
        }
    ], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": 32768, 
                "max_time_min": 300
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw"
        ], 
        "category": "canary"
    }
}
