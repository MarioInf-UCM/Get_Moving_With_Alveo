{
    "gui": false,
    "name": "Xilinx Tree Swaption Engine ECIR Model Benchmark", 
    "description": "", 
    "flow": "vitis", 
    "platform_type": "pcie", 
    "platform_whitelist": [
        "u250", 
        "u200"
    ], 
    "platform_blacklist": [
        "zc"
    ], 
    "platform_properties": {
        "u200": {
            "host": {
                "compiler": {
                    "symbols": [
                        "USE_DDR", 
                        "KN=2"
                    ]
                }
            }, 
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u200.ini"
                    ]
                }
            }
        }, 
        "u250": {
            "host": {
                "compiler": {
                    "symbols": [
                        "USE_DDR", 
                        "KN=4"
                    ]
                }
            }, 
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u250.ini"
                    ]
                }
            }
        }
    }, 
    "launch": [
        {
            "cmd_args": " -xclbin BUILD/scanTreeKernel.xclbin", 
            "name": "generic launch for all flows"
        }
    ], 
    "host": {
        "host_exe": "host.exe", 
        "compiler": {
            "sources": [
                "host/main.cpp", 
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ], 
            "includepaths": [
                "LIB_DIR/L1/include", 
                "LIB_DIR/L2/include", 
                "LIB_DIR/L2/benchmarks/TreeEngine/TreeSwaptionEngineECIRModel/host", 
                "LIB_DIR/L2/benchmarks/TreeEngine/TreeSwaptionEngineECIRModel/kernel", 
                "LIB_DIR/ext/xcl2"
            ], 
            "options": "-O3 ", 
            "symbols": [
                "PRAGMA"
            ]
        }
    }, 
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include", 
                "LIB_DIR/L2/include", 
                "LIB_DIR/L2/benchmarks/TreeEngine/TreeSwaptionEngineECIRModel/kernel"
            ], 
            "symbols": [
                "HW_EMU_DEBUG"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "kernel/scan_tree_kernel1.cpp kernel/scan_tree_kernel.cpp", 
                    "frequency": 300.0, 
                    "clflags": " -D DPRAGMA -D KERNEL_NAME=scanTreeKernel1", 
                    "name": "scanTreeKernel1"
                }, 
                {
                    "location": "kernel/scan_tree_kernel2.cpp", 
                    "frequency": 300.0, 
                    "clflags": " -D DPRAGMA -D KERNEL_NAME=scanTreeKernel2", 
                    "name": "scanTreeKernel2"
                }, 
                {
                    "location": "kernel/scan_tree_kernel3.cpp kernel/scan_tree_kernel.cpp", 
                    "frequency": 300.0, 
                    "clflags": " -D DPRAGMA -D KERNEL_NAME=scanTreeKernel3", 
                    "name": "scanTreeKernel3"
                }, 
                {
                    "location": "kernel/scan_tree_kernel4.cpp kernel/scan_tree_kernel.cpp", 
                    "frequency": 300.0, 
                    "clflags": " -D DPRAGMA -D KERNEL_NAME=scanTreeKernel4", 
                    "name": "scanTreeKernel4"
                }
            ], 
            "ldclflags": " --nk scanTreeKernel1:1:scanTreeKernel1 --nk scanTreeKernel2:1:scanTreeKernel2 --nk scanTreeKernel3:1:scanTreeKernel3 --nk scanTreeKernel4:1:scanTreeKernel4", 
            "frequency": 300.0, 
            "name": "scanTreeKernel"
        }
    ], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": 32768, 
                "max_time_min": 300
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw"
        ], 
        "category": "canary"
    }
}
